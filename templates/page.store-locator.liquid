<header class="page-header" style="border:0 solid #0000;">
  {% if settings.enable_multilang %}
  <h2 class="lang1">{{ page.title | split: '|' | first }}</h2>
  <h2 class="lang2">{{ page.title | split: '|' | last }}</h2>
  {% else %}
  {% comment %}
  <h2>{{ page.title | split: '|' | first }}</h2>
  {% endcomment %}
  {% endif %}
</header>

{% comment %}
Regular page content goes here.
{% endcomment %}

{% if settings.enable_multilang %}
<div class="rte lang1">{{ page.content | split: '[lang2]' | first }}</div>
<div class="rte lang2">{{ page.content | split: '[lang2]' | last }}</div>
{% else %}
<div class="rte">{{ page.content | split: '[lang2]' | first }}</div>
{% endif %}

{{ 'storelocator.css' | asset_url | stylesheet_tag }}

<div class="clearfix"></div>
{% comment %}
Regular page content goes here.
{% endcomment %}
<div class="rte static-page storelocator-panel" style="width:80%;margin:0 auto;">
  <div id="panel"></div>
  <div id="map-canvas"></div>
</div>
{{ page.content }}
